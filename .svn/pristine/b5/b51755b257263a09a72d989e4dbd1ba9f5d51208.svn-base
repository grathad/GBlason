using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace GBlason.Common.Attribute
{
    [AttributeUsage(AttributeTargets.Property)]
    public class CoaPropertyAttribute : System.Attribute
    {
        public string NameResourceKey { get; set; }

        public CoaPropertyAttribute(String nameResourceKey)
        {
            NameResourceKey = nameResourceKey;
        }
    }
}
